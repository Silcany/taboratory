<header class="jumbotron engineering-jumbotron">
  <div class="container text-center">
    <h1><%= @group.name %></h1>
  </div>
</header>


<div class="container">

<% @group.members.each do |member| %>
  <div id='user_inf_window'>
    <%= link_to '', controller: 'members', action: 'show', id: member.id %>
    <div id='thumbnail'>
      <span id="thumbnail_image" style="background-image: url(/images/thumbnail.jpg)"></span>
    </div>
    <div id='user_inf'>
      <div id='name'><%= member.name %></div>
    </div>
  </div>
<% end %>

</div>
